app:
  nome: "Cadastro de Clientes e Produtos"
  objetivo: "Gerenciar duas entidades em telas separadas"

entidade:
  - nome: "cliente"
    campos:
      - nome: texto
      - email: texto
      - telefone: texto

  - nome: "produto"
    campos:
      - nome: texto
      - preco: numero
      - estoque: numero

interface:
  - tela: "Cadastro de Clientes"
    mostra: cliente[]

  - tela: "Cat√°logo de Produtos"
    mostra: produto[]
